<div class="page-header">
  <h1>+ Pic</h1>
</div>

<%= form_for @picture, :url => picture_add_tag_path, :method => "get" do |f| %>

<div class="form-group">
  <%= f.label :url %><br>
  <%= f.text_field :url %>
</div>

<p>
  <%= f.submit 'Confirm', class: 'btn btn-default', data: { disable_with: '...' } %>
</p>

<% end %>
